{
  "root": true,
  "extends": "@akashic/eslint-config",
  "parserOptions": {
    "project": "tsconfig.eslint.json",
    "sourceType": "module"
  },
  "ignorePatterns": [
    "**/*.js"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "args": "none"
      }
    ]
  }
}
